<?xml version="1.0" encoding="windows-1252"?>

<ui:UiBinder
  xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'
  xmlns:view='urn:import:org.adrianwalker.jboard.client.view'>

  <g:HTMLPanel>
    <div class="Wrapper">
      <div class="Header">
        <div class="Title">
          <g:Label ui:field='title' />
        </div>
        <div class="Description">
          <g:Label ui:field='description' />
        </div>
      </div>            
            
      <div class="Container Width420px">
        <div class="ContainerHead">
        </div>
        <div class="ContainerBody">
          <view:SubmitPost ui:field='submit'/>               
        </div>
      </div>
           
      <div class="Container Width100pc">
        <div class="ContainerHead">
        </div>
        <div class="ContainerBody">
          <g:VerticalPanel ui:field='postsPanel'  styleName="Width100pc"/>
        </div>
      </div>
    </div>
  </g:HTMLPanel>
</ui:UiBinder>